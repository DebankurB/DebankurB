public class c(){
public static void main(String[]arg){
int arr[]=new int[10];
Scanner sc=new Scanner(System.in);
for(int i=0;i<arr.length;i++){
arr[i]=sc.nextInt();
}
System.out.println("Enter element to search for");
int n=sc.nextInt();
for(int i=0;i<arr.length;i++){
if(arr[i]==n){
System.out.println("Match found at index"+i);
}}}}
